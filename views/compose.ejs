<%- include("partials/header") -%>
<h1>Compose</h1>
<form action="/" method="post">
  <div class="form-group">
  <label for="bp">title</label>
  <input type="text" name="postName" class="form-control" id="bp" />
</div>
<div class="form-group">
  <label for="ta">post</label>
  <textarea name="postBody" id="ta" cols="30" rows="5" class="form-control"></textarea>
</div>
  <button type="submit" class="btn btn-primary">Publish</button>
</form>
<%- include("partials/footer") -%>
